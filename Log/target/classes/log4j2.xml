<?xml version="1.0" encoding="UTF-8"?>
<Configuration status="WARN">  //OFF  ERROR  WARN  INFO  DEBUG
    <Appenders>
        <Console name="Console" target="SYSTEM_OUT">  //输出到命令行
            <PatternLayout pattern="%d{HH:mm:ss.SSS} [%t] %-5level %logger{36} - %msg%n" />   //定义输出格式
        </Console>
    </Appenders>
    <Loggers>
        <Root level="info">
            <AppenderRef ref="Console" />
        </Root>
    </Loggers>
</Configuration>
